function sendModule() {
    var data = {
        code: editor.getValue(),
        title: $("#moduleTitle").text()
    };

    $.ajax({
        type: "POST",
        url: "@{ModulesR}",
        contentType:'application/json',
        data: JSON.stringify(data),
        dataType:'json',

        success: function () {
            alert("done");
        },
        error: function (x, y, err) {
            alert(err);
        }
    })
}

$("#moduleSave").click(sendModule)

